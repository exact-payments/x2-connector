{
  "name": "@fintechdev/x2-connector",
  "version": "0.3.7",
  "description": "HTTP Connector to communicate with x2 services",
  "main": "dist/x2-connector.js",
  "author": "Nicolas Del Valle <nicolas@fintechdev.net>",
  "contributors": [
    "Ignacio Anaya <ignacio@fintechdev.net>",
    "Robert Hurst <robert@fintechdev.net>"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:dev": "rollup -c",
    "build:dev:w": "rollup -c -w",
    "test": "jest --coverage",
    "check-cover": "open coverage/lcov-report/index.html",
    "check-bundle": "open coverage/bundle-statistics.html",
    "eslint": "eslint . --ext .js"
  },
  "dependencies": {
    "@fintechdev/x2-service-storage": "0.0.2",
    "relign": "^0.5.0",
    "trae": "0.0.10",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "eslint": "^3.9.1",
    "eslint-config-x2": "^2.0.0",
    "fetch-mock": "^5.5.0",
    "jest": "^16.0.2",
    "node-localstorage": "^1.3.0",
    "pre-commit": "^1.1.3",
    "relign": "^0.5.0",
    "rollup": "^0.38.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-conditional": "^0.1.4",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-visualizer": "^0.1.5"
  },
  "pre-commit": [
    "eslint"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "src/**"
    ],
    "testPathDirs": [
      "test/specs/"
    ],
    "setupTestFrameworkScriptFile": "test/setup.js"
  }
}
